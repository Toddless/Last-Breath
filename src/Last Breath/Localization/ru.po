msgid "ExtraArmor"
msgstr "Дополнительная броня"

# Extra Energy Barrier
msgid "ExtraEnergyBarrier"
msgstr "Дополнительный энергетический барьер"

# Extra Evade
msgid "ExtraEvade"
msgstr "Дополнительное уклонение"

# Extra Damage
msgid "ExtraDamage"
msgstr "Дополнительный урон"

# Extra Suppress
msgid "ExtraSuppress"
msgstr "Дополнительное подавление"

# Extra Spell Damage
