# ------------------------------
# Parameters
# ------------------------------

#. Player damage stat
msgid "Damage"
msgstr "Урон"

#. Player health stat
msgid "Health"
msgstr "Здоровье"

#. Player critical chance
msgid "CriticalChance"
msgstr "Шанс критического удара"

#. Maximum possible critical chance
msgid "MaxCriticalChance"
msgstr "Макс. шанс критического удара"

#. Critical damage multiplier
msgid "CriticalDamage"
msgstr "Критический урон"

#. Additional hit chance
msgid "AdditionalHitChance"
msgstr "Шанс дополнительного удара"

#. Maximum additional hit chance
msgid "MaxAdditionalHitChance"
msgstr "Макс. шанс дополнительного удара"

#. Current health value
msgid "CurrentHealth"
msgstr "Текущее здоровье"

#. Maximum health
msgid "MaxHealth"
msgstr "Макс. здоровье"

#. Dexterity attribute
msgid "Dexterity"
msgstr "Ловкость"

#. Strength attribute
msgid "Strength"
msgstr "Сила"

#. Intelligence attribute
msgid "Intelligence"
msgstr "Интеллект"

#. Armor stat
msgid "Armor"
msgstr "Броня"

#. Evasion stat
msgid "Evade"
msgstr "Уклонение"

#. Magical barrier
msgid "EnergyBarrier"
msgstr "Энергетический барьер"

#. Spell damage stat
msgid "SpellDamage"
msgstr "Урон от заклинаний"

#. Maximum resource (mana, energy etc.)
msgid "ResourceMax"
msgstr "Макс. ресурс"

#. Resource recovery per turn
msgid "ResourceRecovery"
msgstr "Восстановление ресурса"

#. Maximum damage reduction cap
msgid "MaxReduceDamage"
msgstr "Макс. снижение урона"

#. Movement speed
msgid "Movespeed"
msgstr "Скорость передвижения"

#. Maximum evade chance
msgid "MaxEvadeChance"
msgstr "Макс. шанс уклонения"

#. Suppression stat
msgid "Suppress"
msgstr "Подавление"

#. All attributes (universal modifier)
msgid "AllAttribute"
msgstr "Все атрибуты"

# ------------------------------
# Modifier Types
# ------------------------------

#. Flat modifier: +300 HP, +15 Def, etc.
msgid "Flat"
msgstr "Фиксированный"

#. Increase modifier: percent values like 0.1, 0.3
msgid "Increase"
msgstr "Увеличение"

#. Multiplicative modifier: values like 1.3, 1.1
msgid "Multiplicative"
msgstr "Множитель"

# ------------------------------
# Modifier Format String
# ------------------------------

#. Template for showing modifiers: {0} = parameter, {1} = value
msgid "modifier.format"
msgstr "{0}: {1}"


