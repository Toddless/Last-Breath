QuestsManager="*res://Script/QuestSystem/QuestManager.cs"
ItemsMediaHandler="*res://Script/Items/ItemData/ItemsMediaHandler.cs"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="LastBreath"

[editor_plugins]

enabled=PackedStringArray("res://addons/Crafting/plugin.cfg")

[file_customization]

folder_colors={
"res://Components/": "purple",
"res://Resource/": "green",
"res://Scenes/": "orange",
"res://Script/Enemy/": "green",
"res://Script/Helpers/": "green",
"res://Script/Inventory/": "red",
"res://Script/Items/": "orange",
"res://Script/LootGenerator/": "orange",
"res://Script/UI/": "red"
}

